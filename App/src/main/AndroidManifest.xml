        <activity android:name=".home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".basic.AndroidBasicActivity" />
        <activity android:name=".view.MyViewActivity" />
        <activity android:name=".database.MyDataBaseActivity" />
        <activity android:name=".network.MyNetWorkActivity" />
        <activity android:name=".kotlin.MyKotlinActivity" />
        <activity android:name=".image.MyImageActivity" />
        <activity android:name=".modle.MyModeFrameActivity" />
