        <activity android:name=".modle.mvc.control.MyMVCActivity" />
        <activity android:name=".modle.mvvm.MyMVVMActivity" />
        <activity android:name=".image.picasso.MyPicassoActivity" />
        <activity android:name=".image.glide.MyGlideActivity" />
        <activity android:name=".network.retrofit.rxjava.MyRxjavaActivity"></activity>
        <activity android:name=".image.fresco.MyFrescoActivity" />
        <activity android:name=".network.okhttp.MyOkhttpActivity" />
        <activity android:name=".network.retrofit.MyRetrofitActivity" />
        <activity android:name=".network.retrofit.MyOnlyRetrofitActivity" />
        <activity android:name=".network.retrofit.MyRetrofitRxjavaActivity" />
        <activity android:name=".modle.MyFrameActivity" />
        <activity android:name=".network.asynchttp.MyAsyncTaskActivity" />
        <activity android:name=".view.MyWebViewActivity" />
